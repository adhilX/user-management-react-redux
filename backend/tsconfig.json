  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./src",
    "types": ["node"],
    "strict":true,
    "forceConsistentCasingInFileNames":true,
    "typeRoots": ["./node_modules/@types", "./types"]
  }
}